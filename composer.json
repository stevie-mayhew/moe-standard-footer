{
    "name": "education-nz/moe-standard-footer",
    "description": "Common footer styles for education.govt.nz",
    "type": "silverstripe-vendormodule",
    "license": "BSD-3-Clause",
    "autoload": {
        "psr-4": {
            "Education\\StandardFooter\\": "src/",
            "Education\\StandardFooter\\Tests\\": "tests/"
        }
    },
    "require": {
        "education-nz/silverstripe-core": "^2",
        "education-nz/moe-design-system": "^1"
    },
    "extra": {
        "expose": [
            "client"
        ],
        "branch-alias": {
            "dev-main": "1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "lint": "phpcs src/ tests/",
        "lint-clean": "phpcbf src/ tests/"
    }
}
